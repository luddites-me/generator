    "@types/mocha": "7.0.2",
    "@types/node": "13.9.0",
    "@types/semver": "7.1.0",
    "@types/uuid": "7.0.0",
    "@typescript-eslint/eslint-plugin": "2.22.0",
    "@typescript-eslint/parser": "2.22.0",
    "archiver": "3.1.1",
    "awesome-typescript-loader": "5.2.1",
    "aws-lambda": "1.0.5",
    "aws-sdk": "2.635.0",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "create-ts-index": "1.12.0",
    "dotenv-extended": "2.7.1",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-tsdoc": "0.2.3",
    "hard-source-webpack-plugin": "0.13.1",
    "mocha": "7.1.0",
    "nyc": "15.0.0",
    "prettier": "1.19.1",
    "scp2": "0.5.0",
    "semver": "7.1.3",
    "serverless": "1.65.0",
    "serverless-pseudo-parameters": "2.5.0",
    "serverless-step-functions": "2.17.2",
    "ts-mocha": "6.0.0",
    "ts-node": "8.6.2",
    "typescript": "3.8.3",
    "webpack": "4.42.0",
    "webpack-cli": "3.3.11",
    "webpack-merge": "4.2.2"
  },
  "files": [
    "dist"
  ],
  "license": "proprietary",
  "main": "dist/switchboard.min.js",
  "name": "@ns8/<%- platformName %>-platform",
  "private": true,
  "scripts": {
    "beautify": "ts-node -P ./build/tsconfig.json ./build/projectBuild.ts",
    "build": "yarn && yarn generate:exports && yarn beautify && tsc --emitDeclarationOnly && yarn bundle",
    "build:dev": "NODE_ENV=dev yarn build",
    "build:prod": "NODE_ENV=prod yarn build",
    "bundle": "webpack",
    "clean": "rm -rf .awcache dist .serverless release",
    "deploy": "METHOD=deploy ts-node -P ./build/tsconfig.json ./build/lambdaDeploy.ts",
    "generate:exports": "cti create ./switchboard -w",
    "lint": "eslint --ext=ts switchboard build",
    "lint:fix": "yarn lint --fix",
    "lint:markdown": "eslint --ext md .",
    "rebuild": "yarn clean && yarn build",
    "rebuild:dev": "yarn clean && yarn build:dev",
    "test": "nyc mocha 'switchboard/**/*test.ts'",
    "test:debug": "mocha -r ts-node/register 'switchboard/**/*test.ts'",
    "undeploy": "METHOD=remove ts-node -P ./build/tsconfig.json ./build/lambdaDeploy.ts",
    "version:patch": "ts-node -P ./build/tsconfig.json ./build/incrementVersion.ts && yarn build && yarn publish",
    "version:patch:dev": "PATCH_MODE=dev yarn version:patch",
    "version:publish": "yarn && yarn build && npm publish"
  },
  "types": "dist/types/index.d.ts",
  "version": "0.0.1"
}
