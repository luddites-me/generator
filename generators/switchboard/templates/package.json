  "license": "proprietary",
  "main": "dist/switchboard.min.js",
  "name": "@ns8/<%- platformName %>-platform",
  "private": true,
  "scripts": {
    "beautify": "ts-node ./build/projectBuild.ts",
    "build": "yarn && yarn generate:exports && yarn beautify && tsc --emitDeclarationOnly && yarn bundle",
    "build:dev": "NODE_ENV=dev yarn build",
    "build:prod": "NODE_ENV=prod yarn build",
    "bundle": "webpack",
    "clean": "rm -rf .awcache dist .serverless",
    "deploy": "METHOD=deploy ts-node ./build/lambdaDeploy.ts",
    "generate:exports": "yarn cti create ./switchboard -w",
    "lint": "eslint --ext=ts switchboard",
    "lint:fix": "yarn lint --fix",
    "rebuild": "yarn clean && yarn build",
    "rebuild:dev": "yarn clean && yarn build:dev",
    "test": "nyc mocha 'test/**/*Test.ts'",
    "test:debug": "mocha -r ts-node/register 'test/**/*Test.ts'",
    "undeploy": "METHOD=remove yarn ts-node ./build/lambdaDeploy.ts",
    "version:patch": "yarn ts-node ./build/incrementVersion.ts && yarn build && yarn publish",
    "version:patch:dev": "PATCH_MODE=dev yarn version:patch"
  },
  "types": "dist/types/index.d.ts",
  "version": "0.0.1"
}
