{
  "name": "@ns8/generator-protect-integration",
  "version": "0.0.12",
  "description": "Protect integration project generator",
  "homepage": "https://github.com/ns8inc/generator-protect-integration",
  "author": {
    "name": "ns8inc",
    "url": "https://ns8.com"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "ns8",
    "protect",
    "yeoman-generator"
  ],
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-config-xo": "^0.27.2",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "jest": "^24.8.0",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "engines": {
    "node": ">= 8.2.1",
    "npm": ">= 5.3.0"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "lodash": "^4.17.15",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "/__tests__/.*"
  },
  "lint-staged": {
    "*.js": [
      "eslint -c package.json --no-eslintrc --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": "https://github.com/ns8inc/protect-project-generator"
}
